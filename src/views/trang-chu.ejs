  <script>
    $('.quick-view-btn').click((event) => {
      var productID = $(event.currentTarget).data('product-id');
      console.log(productID);
      showProductDetail(productID);
    })
    const showProductDetail = (productId) => {
      $.ajax({
        url: '/review' + productId,
        method: 'GET',
        success: (product) => {
          console.log(product);
          let productModal = $('#productModal')
          for (i = 0; i < product.length; i++) {
            if (product[i].Other === "C") {
              productModal.find('.js-name-detail').text(product[i].tengiay)
              productModal.find('.product-price').text('VNĐ' + product[i].gia)
              productModal.find('.product-image').attr('src', `img/${product[i].hinhanh}`)
              productModal.find('.product-image-link').attr('href', `img/${product[i].hinhanh}`)
              productModal.find(`.item-slick3-${i}`).attr('data-thumb', `img/${product[i].hinhanh}`);
              productModal.find('.IDsanpham').attr('value', product[i].Magiay)
            } else {

              productModal.find(`.product-image-${i}`).attr('src', `img/${product[i].hinhanh}`)
              productModal.find(`.product-image-link-${i}`).attr('href', `img/${product[i].hinhanh}`)
              productModal.find(`.item-slick3-${i}`).attr('data-thumb', `img/${product[i].hinhanh}`);
            }
          }
          $('.js-show-modal1').on('click', function (e) {
            e.preventDefault();
            $('.js-modal1').addClass('show-modal1');
          });
          $('.js-hide-modal1').on('click', function () {
            $('.js-modal1').removeClass('show-modal1');
          });
        }
      })
    }
  </script>

  <script>
    const copyButtons = document.querySelectorAll('.right');
    copyButtons.forEach(copyButton => {
      copyButton.addEventListener('click', function () {
        // Get the coupon code from the <span> element
        const couponCode = this.previousElementSibling.querySelector('b').textContent;

        // Copy the coupon code to clipboard
        navigator.clipboard.writeText(couponCode).then(function () {
          // Show success message
          Swal.fire({
            icon: 'success',
            title: 'Coupon code copied!',
            showConfirmButton: false,
            timer: 1500
          });
        });
      });
    });
  </script>
